							{
								"activity": "Get Measurement Folders",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@activity('Get Measurement Folders').output.childItems",
								"type": "Expression"
